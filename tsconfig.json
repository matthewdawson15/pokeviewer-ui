{
  "compilerOptions": {
    "outDir": "./built",
    "target": "ES6",
    "jsx": "preserve",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "removeComments": true,
    "alwaysStrict": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "allowUnreachableCode": false
  },
  "include": ["./src/**/*"]
}
